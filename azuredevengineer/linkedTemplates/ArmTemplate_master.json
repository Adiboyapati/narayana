{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredevengineer"
		},
		"AzureBlobStorage_adfdevelopment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_adfdevelopment'"
		},
		"AzureDataLakeStorage_adfdevelopment_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_adfdevelopment'"
		},
		"AzureDataLakeStorage_adfdevelopment_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuredatalakestoragee.dfs.core.windows.net/"
		},
		"azuredatacopy_adls_triggers_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/007dcd1d-92b6-47f6-a5f4-a566d7fdef64/resourceGroups/azuredevelopment-rg/providers/Microsoft.Storage/storageAccounts/azureblobstoragee"
		},
		"adf_blobread_to_loadADLS_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/007dcd1d-92b6-47f6-a5f4-a566d7fdef64/resourceGroups/azuredevelopment-rg/providers/Microsoft.Storage/storageAccounts/azureblobstoragee"
		},
		"adf_schedule_triggers_properties_blobToadls_getmetadata_lookup_parameters_Filaname": {
			"type": "string",
			"defaultValue": "Customer_ABC_Items.xls"
		},
		"Adls_Tumble_triggers_properties_pipeline_parameters_Filename": {
			"type": "string",
			"defaultValue": "Customer_ABC_Items.xls"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredevengineer_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_adfdevelopment_connectionString": {
						"value": "[parameters('AzureBlobStorage_adfdevelopment_connectionString')]"
					},
					"AzureDataLakeStorage_adfdevelopment_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_adfdevelopment_accountKey')]"
					},
					"AzureDataLakeStorage_adfdevelopment_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_adfdevelopment_properties_typeProperties_url')]"
					},
					"azuredatacopy_adls_triggers_properties_typeProperties_scope": {
						"value": "[parameters('azuredatacopy_adls_triggers_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "azuredevengineer_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adf_blobread_to_loadADLS_properties_typeProperties_scope": {
						"value": "[parameters('adf_blobread_to_loadADLS_properties_typeProperties_scope')]"
					},
					"adf_schedule_triggers_properties_blobToadls_getmetadata_lookup_parameters_Filaname": {
						"value": "[parameters('adf_schedule_triggers_properties_blobToadls_getmetadata_lookup_parameters_Filaname')]"
					},
					"Adls_Tumble_triggers_properties_pipeline_parameters_Filename": {
						"value": "[parameters('Adls_Tumble_triggers_properties_pipeline_parameters_Filename')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azuredevengineer_ArmTemplate_0')]"
			]
		}
	]
}